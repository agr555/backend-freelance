{
    "version": 2,
    "builds": [
        {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
        "src": "public/**",
        "use": "@vercel/static"

    }
],
    "routes": [
      {
        "src": "/api/",
        "dest": "index.js"
      }, 
      {
        "src": "/api/cat",
        "dest": "index.js"
      },
    
      {
        "src": "/api/m",
        "dest": "migrate-mongo-config.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      }, 
      {
        "src": "/",
        "dest": "public/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      }
    ]
  }